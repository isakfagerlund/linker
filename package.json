    "url": "https://github.com/isakfagerlund/linker/issues"
  },
  "homepage": "https://github.com/isakfagerlund/linker#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "express": "^4.16.2",
    "nodemon": "^1.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0",
    "concurrently": "^3.5.0"
  }
