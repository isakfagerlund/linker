{
  "name": "linker",
  "version": "0.1.0",
  "description": "A link shortener",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "client": "cd client && npm start",
    "server": "nodemon --ignore client/ start.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test": "jest __tests__/*.test.js",
    "client-test": "cd client && npm run test",
    "postinstall": "cd client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isakfagerlund/linker.git"
  },
  "author": "Isak Fagerlund",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isakfagerlund/linker/issues"
  },
  "homepage": "https://github.com/isakfagerlund/linker#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "jest": "^22.4.2",
    "mongoose": "^5.0.7",
    "nodemon": "^1.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "supertest": "^3.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0"
  }
}
