{
  "name": "linker",
  "version": "0.1.0",
  "description": "A link shortener",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "client": "cd client && yarn start",
    "server": "nodemon --ignore client/ start.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
